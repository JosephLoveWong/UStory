<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundedFrameLayout">
        <attr name="corner_radius" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="USImageView">
        <attr name="background" format="reference"/>
        <attr name="isCacheAll" format="boolean"/>
        <attr name="dontTransform" format="boolean"/>
        <attr name="orientation" format="integer">
            <enum name="original" value="1"/>
            <enum name="landscape" value="2"/>
            <enum name="portrait" value="3"/>
            <enum name="quad" value="4"/>
        </attr>
        <attr name="us_size" format="integer">
            <enum name="full" value="0"/>
            <enum name="large" value="1"/>
            <enum name="medium" value="2"/>
            <enum name="small" value="3"/>
            <enum name="icon" value="4"/>
        </attr>
        <attr name="us_size_landscape" format="integer">
            <enum name="full" value="0"/>
            <enum name="large" value="1"/>
            <enum name="medium" value="2"/>
            <enum name="small" value="3"/>
            <enum name="icon" value="4"/>
        </attr>
        <attr name="us_size_portrait" format="integer">
            <enum name="full" value="0"/>
            <enum name="large" value="1"/>
            <enum name="medium" value="2"/>
            <enum name="small" value="3"/>
            <enum name="icon" value="4"/>
        </attr>
    </declare-styleable>
</resources>